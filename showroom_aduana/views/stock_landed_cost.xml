<odoo>
  <data>
    <record id="stock_landed_cost_inherit_showroom_aduana" model="ir.ui.view">
      <field name="name">stock.landed.cost.inherit.showroom.aduana</field>
      <field name="model">stock.landed.cost</field>
      <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form"/>
      <field name="arch" type="xml">
        
        <xpath expr="//field[@name='date']" position="after">
          <field name="cost_aduana"/>
        </xpath>
    
      </field>
    </record>
  </data>
</odoo>